{
    "env": {
        "node": true,
        "browser": false,
        "commonjs": false,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "quotes":["error", "single"],
        "no-underscore-dangle": [ "error", { "allow": ["_id"] } ]
    }
}