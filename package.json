{
  "name": "tutor-assistant-api",
  "version": "1.0.0",
  "description": "manage student data and provide email templates for all processes",
  "main": "server/server.js",
  "scripts": {
    "test": "npm run lint && npm run mocha",
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server/index.js",
    "start:dev": "concurrently \"npm run server\" \"npm run client\"",
    "client": "cd client && npm run start",
    "server": "cd server && npm run dev",
    "lint": "eslint --quiet .",
    "fix": "eslint --fix .",
    "install": "cd server && npm i && cd ../client && npm i"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/samuelfox1/tutor-assistant-api.git"
  },
  "author": "samuel fox & daniel soledad",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/samuelfox1/tutor-assistant-api/issues"
  },
  "homepage": "https://github.com/samuelfox1/tutor-assistant-api#readme",
  "devDependencies": {
    "concurrently": "^6.2.2"
  },
  "dependencies": {
    "bulma-slider": "^2.0.4",
    "compression": "^1.7.4",
    "node-sass": "^6.0.1"
  }
}